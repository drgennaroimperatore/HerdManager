<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimary">

    <TextView
        android:id="@+id/textView29"
        style="@style/BlackHeadingText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Use Existing Farmer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.ilri.herdmanager.ui.customui.CustomRadioGroup
        android:id="@+id/existing_farmer_RadioGroup"
        android:layout_width="411dp"
        android:layout_height="547dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView29"
        app:layout_constraintVertical_bias="0.495">

        <com.ilri.herdmanager.ui.customui.FarmerRadioButton
            android:id="@+id/existingFarmerRadioButton"
            android:layout_width="136dp"
            android:layout_height="97dp"
            android:background="@drawable/farmer_radio_button_unselected_state"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

        </com.ilri.herdmanager.ui.customui.FarmerRadioButton>

        <com.ilri.herdmanager.ui.customui.FarmerRadioButton
            android:id="@+id/existingFarmerRadioButton2"
            android:layout_width="136dp"
            android:layout_height="97dp"
            android:background="@drawable/farmer_radio_button_unselected_state"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/existingFarmerRadioButton"
            app:layout_constraintTop_toTopOf="parent">

        </com.ilri.herdmanager.ui.customui.FarmerRadioButton>

    </com.ilri.herdmanager.ui.customui.CustomRadioGroup>


</androidx.constraintlayout.widget.ConstraintLayout>