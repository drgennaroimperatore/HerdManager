<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


    <TextView
        android:id="@+id/health_intervention_row_name_textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:text="Intervention Name"
        android:gravity="center"
        style="@style/RowHeaderTextStyle">
    </TextView>

        <TextView
            android:id="@+id/health_intervention_row_vaccination_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.5"
            style="@style/RowSubHeaderTextStyle"
            android:text="">
        </TextView>
    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/health_event_health_intervention_row_babies_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            style="@style/RowSubHeaderTextStyle"
            android:text="P.Wnd"
            android:visibility="gone">
        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="Young"
            style="@style/RowSubHeaderTextStyle"
            android:visibility="gone">
        </TextView>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="Adult"
            style="@style/RowSubHeaderTextStyle"
            android:visibility="gone">
        </TextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/health_intervention_row_nAffectedBabies_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            style="@style/RowContentTextStyle"
            android:gravity="center"
            android:text="0">
        </TextView>

        <TextView
            android:id="@+id/health_intervention_row_nAffectedYoung_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="0"
            android:gravity="center"
            style="@style/RowContentTextStyle">
        </TextView>

        <TextView
            android:id="@+id/health_intervention_row_nAffectedOld_textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="0.3"
            android:text="0"
            android:gravity="center"
            style="@style/RowContentTextStyle">
        </TextView>
    </LinearLayout>

</LinearLayout>